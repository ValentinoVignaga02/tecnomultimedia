

void setup(){

size(400,400);
  
  background(255);
  strokeWeight(5);
  
  
}

void draw(){
background(255);
  for( int i = 10; i<400; i = i+10){
    noFill();  
    circle( 200,200,i);
  }

    


}


void keyPressed(){

if ( key == ' ');{
stroke(random(0,255), random(0,255), random(0,255));
circle(200,200,20);



}
  
}
